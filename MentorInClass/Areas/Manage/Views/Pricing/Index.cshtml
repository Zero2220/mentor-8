
@model List<Card>
<table class="table">
    <a asp-controller="Pricing" asp-action="Create" ><button type="button" class="btn btn-primary">Create</button></a>
    <thead>
        <tr>
            
                <th scope="col">Id</th>
                <th scope="col">Name</th>
                <th scope="col">Price</th>
                <th scope="col">Is Advanced</th>
                <th scope="col">Is Featured </th>
                <th scope="col">Action </th>

            
        </tr>
    </thead>
    <tbody>
    
        
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.Name</td>
                <td>@item.Price</td>
                @if (item.IsAdvanced)
                {
                    <td>Advenced</td>
                }
                else{
                    <td>Not advenced</td>
                }

                @if (item.IsFeatured)
                {
                    <td>Featured</td>
                }
                else{
                    <td>Not Featured</td>
                }

                
                <td><a><button type="button" class="btn btn-warning">Edit</button></a></td>
            </tr>
        }

    </tbody>
</table>